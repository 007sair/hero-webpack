{
  "name": "hero-webpack",
  "version": "1.1.0",
  "description": "hero by webpack",
  "scripts": {
    "build": "webpack --config ./build/webpack.prod.config.js --bail",
    "dev": "webpack-dev-server --inline --color --config ./build/webpack.dev.config.js",
    "dll": "webpack --colors --config ./build/webpack-dll.config.js",
    "start": "webpack-dev-server --open --hot --color",
    "all": "npm run dll && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:007sair/hero-webpack.git"
  },
  "author": "longchan",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-include-assets-plugin": "^1.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.2",
    "postcss-csso": "^3.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-pxtorem": "^4.0.1",
    "postcss-short": "^4.1.0",
    "postcss-sprites": "^4.2.1",
    "postcss-use": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylefmt": "^6.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  }
}
